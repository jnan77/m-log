<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="zh_CN" lang="zh_CN">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
		<meta http-equiv="Content-Language" content="zh_CN" />
		<title>${blogname}</title>
		<link rel="stylesheet" type="text/css" href="${template_url}/style/style.css"  media="screen" />
		<link rel="alternate" type="application/rss+xml" href="${base}/rss.xml" title="${blogname}" />
		<link rel="alternate" type="application/atom+xml" href="${base}/atom.xml" title="${blogname}" />
		<link rel="shortcut icon" href="${template_url}/images/favicon.ico" />
	</head>
	<body class="multi default">
		<!-- Layout -->
		<div id="header-region" class="clear-block"></div>
		<div id="wrapper">
			<div id="container" class="clear-block">
				<div id="header">
					<div id="logo-floater">
						<h1>
							<a href="${base}" title="${blogname}"><img src="${template_url}/images/logo.png" alt="${blogname}" id="logo"><span>${blogname}</span></a>
						</h1>
					</div>
					<div id="primary-menu">
						<ul class="links primary-links">
							
						</ul>
					</div>
				</div>
				<!-- /header -->
				<div id="center">
					<div id="squeeze">
						<div class="right-corner">
							<div class="left-corner">
								<div id="mission">${notice}</div>