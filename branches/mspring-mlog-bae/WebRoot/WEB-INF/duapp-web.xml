<?xml version="1.0" encoding="utf-8"?>
<du-web-app xmlns="http://bae.baidu.com/java/1.0">
	<!-- Configure java.util.logging -->
	<system-properties>
		<property name="app.name" value="MLOG" />
		<property name="app.version" value="1.2 BAE" />
		<!-- 加入性能分析的包路径 -->
		<property name="package_filter" value="org.mspring"/>
	</system-properties>
	<sessions-enabled>true</sessions-enabled>
	<static-files>
		<exclude path="/**.html" />
		<exclude path="/**.js" />
	</static-files>
</du-web-app>
